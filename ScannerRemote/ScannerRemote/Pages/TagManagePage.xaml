<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ScannerRemote.Pages.TagManagePage">
  <StackLayout>
    <TableView>
      <TableView.Root>
        <TableRoot>
          <TableSection Title="Tag to add">
            <EntryCell Label="Add Tag:" Text="{Binding NewEntry.TagName, Mode=TwoWay }" Placeholder="Enter a new Tag" />
          </TableSection>
        </TableRoot>
      </TableView.Root>
    </TableView>
  </StackLayout>
</ContentPage>